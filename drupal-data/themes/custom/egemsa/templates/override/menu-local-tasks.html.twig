<div class="container">
{% if primary %}
  <h2 class="visually-hidden">{{ 'Primary tabs'|t }}</h2>
  <nav>
  <ul class="nav nav-tabs primary">{{ primary }}</ul>
  </nav>
{% endif %}
{% if secondary %}
  <h2 class="visually-hidden">{{ 'Secondary tabs'|t }}</h2>
  <nav>
  <ul class="nav nav-tabs secondary">{{ secondary }}</ul>
  </nav>
{% endif %}
</div>